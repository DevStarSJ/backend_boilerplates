{
   "type": "mysql",
   "host": "localhost",
   "port": 3306,
   "username": "root",
   "password": "admin",
   "database": "dev",
   "synchronize": false,
   "migrationsRun": true,
   "logging": ["query", "error"],
   "entities": [
      "dist/entity/**/*.js"
   ],
   "migrations": [
      "dist/migration/**/*.js"
   ],
   "subscribers": [
      "dist/subscriber/**/*.js"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   },
   "extra": {
      "connectionLimit": 2
   }
}